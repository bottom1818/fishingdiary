<div class="container">
  <u><h2>ログイン</h2></u>
  <%= form_with(scope: :session, url: sessions_path) do |f| %>
    <ul>
      <li>
        <%= f.label :email %>
        <%= f.email_field :email %>
      </li>
      <li>
        <%= f.label :password %>
        <%= f.password_field :password %>
      </li>
    </ul>
    <%= f.submit "ログイン" %>
  <% end %>
</div>
